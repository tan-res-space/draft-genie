{
  "rabbit_version": "3.13.0",
  "rabbitmq_version": "3.13.0",
  "product_name": "RabbitMQ",
  "product_version": "3.13.0",
  "users": [],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "internal_cluster_id",
      "value": "rabbitmq-cluster-id-draft-genie"
    }
  ],
  "policies": [],
  "queues": [
    {
      "name": "speaker.events",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "quorum"
      }
    },
    {
      "name": "draft.events",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "quorum"
      }
    },
    {
      "name": "rag.events",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "quorum"
      }
    },
    {
      "name": "evaluation.events",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "quorum"
      }
    }
  ],
  "exchanges": [
    {
      "name": "draft-genie.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "draft-genie.events",
      "vhost": "/",
      "destination": "speaker.events",
      "destination_type": "queue",
      "routing_key": "speaker.*",
      "arguments": {}
    },
    {
      "source": "draft-genie.events",
      "vhost": "/",
      "destination": "draft.events",
      "destination_type": "queue",
      "routing_key": "draft.*",
      "arguments": {}
    },
    {
      "source": "draft-genie.events",
      "vhost": "/",
      "destination": "rag.events",
      "destination_type": "queue",
      "routing_key": "rag.*",
      "arguments": {}
    },
    {
      "source": "draft-genie.events",
      "vhost": "/",
      "destination": "evaluation.events",
      "destination_type": "queue",
      "routing_key": "evaluation.*",
      "arguments": {}
    }
  ]
}

